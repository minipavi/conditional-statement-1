#include <stdio.h>
int main(void){
int num;
printf("Enter your mark");
scanf("%d",&num);
printf("you entered %d",num);
   
     if (num>=85 && 100){
     printf("you got A grade ");
     {
     else if (num >=70 && 84 ){
     print("you got B grade ");
     }
     else if (num >=55 && 69 ){
     print("you got C grade ");
     }
     else if (num >=40 && 54 ){
     print("you got D grade ");
     }
     else if (num <=40 ){
     print("you got F grade ");
     }
return 0;
}
